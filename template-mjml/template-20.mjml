<mjml>
    <mj-head>
        <mj-include path="./head-style.mjml" />
        <mj-title>Email title</mj-title>
        <mj-preview>Preview text email</mj-preview>
        <mj-attributes>            
            <mj-all font-family="Helvetica" />            
        </mj-attributes>
    </mj-head>

    <mj-body background-color="#F6F7FA">
        
        <mj-include path="./header.mjml" />

        <mj-section padding-top="24px" padding-bottom="24px">
            <mj-column>
                <mj-text css-class="test-dark-mode" padding="0" align="center" color="#92A1B2" font-size="14px" font-style="normal" font-weight="700" line-height="20px">
                    THINGS TO DO, REMINDER                    
                </mj-text>
            </mj-column>
        </mj-section>

        <mj-wrapper background-color="#EDEDED" border-radius="17px" padding="1px">
        <mj-section background-color="#FFF" border-radius="16px" padding-top="32px" padding-right="16px" padding-left="16px">
            
            <mj-group width="100%">
                <mj-column width="23%" padding-left="0px">
                    <mj-image padding-left="0px" padding-right="0px" padding-top="0px" padding-bottom="0px" align="center" width="32px" src="https://itsblog.ru/email/icon_v.png" />
                </mj-column>
                <mj-column width="4%">
                    <mj-image padding-left="0px" padding-right="0px" padding-top="8px" padding-bottom="8px" align="center" width="20px" src="https://itsblog.ru/email/arrow-2.png" />
                </mj-column>
                <mj-column width="22%">
                    <mj-image padding-left="0px" padding-right="0px" padding-top="0px" padding-bottom="0px" align="center"  width="32px" src="https://itsblog.ru/email/icon_v.png" />
                </mj-column>
                <mj-column width="4%">
                    <mj-image padding-left="0px" padding-right="0px" padding-top="8px" padding-bottom="8px" align="center" width="20px" src="https://itsblog.ru/email/arrow-2.png" />
                </mj-column>
                <mj-column width="18%">
                    <mj-image padding-left="0px" padding-right="0px" padding-top="0px" padding-bottom="0px" align="center"  width="32px" src="https://itsblog.ru/email/icon_v.png" />
                </mj-column>
                <mj-column width="4%">
                    <mj-image padding-left="0px" padding-right="0px" padding-top="8px" padding-bottom="8px" align="center" width="20px" src="https://itsblog.ru/email/arrow-2.png" />
                </mj-column>
                <mj-column width="23%" padding-right="0px">
                    <mj-image padding-left="30px" padding-right="0px" padding-top="0px" padding-bottom="0px" align="center"  width="32px" src="https://itsblog.ru/email/finish-1.png" />
                </mj-column>
            </mj-group>            

            <mj-group width="100%">
                <mj-column width="25%" padding-left="0px" padding-top="16px">
                    <mj-text padding-left="0px" padding-right="0px" padding-top="0px" padding-bottom="0" align="center" color="#212429" font-size="12px" font-style="normal" font-weight="400" line-height="16px">Survey to employee<br> & manager</mj-text>
                </mj-column>
                <mj-column width="25%" padding-top="16px">
                    <mj-text padding-left="0px" padding-right="0px" padding-top="0px" padding-bottom="0" align="center" color="#212429" font-size="12px" font-style="normal" font-weight="400" line-height="16px">Suggestion<br> to manager</mj-text>
                </mj-column>
                <mj-column width="25%" padding-top="16px">
                    <mj-text padding-left="0px" padding-right="0px" padding-top="0px" padding-bottom="0" align="center" color="#212429" font-size="12px" font-style="normal" font-weight="400" line-height="16px">Second Survey to<br> employee</mj-text>
                </mj-column>
                <mj-column width="25%" padding-right="0px" padding-top="16px" vertical-align="middle">
                    <mj-text  padding-left="20px" padding-right="0px" padding-top="5px" padding-bottom="0" align="center" color="#212429" font-size="12px" font-style="normal" font-weight="400" line-height="16px">Results</mj-text>
                </mj-column>
            </mj-group>

        </mj-section>
        </mj-wrapper>

        <mj-section padding="0px">
        <mj-column>
            <mj-spacer height="16px"/>            
        </mj-column>
        </mj-section>

        <mj-wrapper background-color="#EDEDED" border-radius="17px" padding="1px">
            <mj-section background-color="#FFF" padding-right="32px" border-radius="16px" padding-top="32px" padding-bottom="32px" background-url="https://itsblog.ru/email/border-image-left.png" background-repeat="no-repeat" background-position-x="left" background-position-y="32px" background-size="4px 48px">
            
                <mj-group width="100%">                
                    <mj-column>                    
                        <mj-text padding-left="32px" padding-right="0px" padding-top="0px" padding-bottom="0" align="left" color="#92A1B2" font-size="12px" font-style="normal" font-weight="400" line-height="16px">May 2022</mj-text>                
                        <mj-text padding-left="32px" padding-right="0px" padding-top="4px" padding-bottom="0" align="left" color="#212429" font-size="20px" font-style="normal" font-weight="700" line-height="28px">Your Monthly Results for May now available</mj-text>
                        <mj-divider border-width="1px" border-style="solid" border-color="#EDEDED" padding-left="0px" padding-right="0px" padding-top="32px" padding-bottom="32px" />
                    </mj-column>
                </mj-group>
                <mj-group width="100%">
                    <mj-column>
                        <mj-text padding-left="32px" padding-right="0px" padding-top="0px" padding-bottom="0" align="left" color="#212429" font-size="14px" font-style="normal" font-weight="400" line-height="20px">Actual saving</mj-text>
                        <mj-text padding-left="32px" padding-right="0px" padding-top="8px" padding-bottom="0" align="left" color="#212429" font-size="32px" font-style="normal" font-weight="700" line-height="40px">$50,000</mj-text>
                        <mj-text padding-left="32px" padding-right="0px" padding-top="8px" padding-bottom="0" align="left" color="#92A1B2" font-size="14px" font-style="normal" font-weight="400" line-height="20px">Predicted total saving â€” $600,000</mj-text>
                        <mj-button width="100%" border-radius="8px" padding-top="20px" padding-bottom="0" padding-left="32px" padding-right="0px" font-family="Helvetica" font-size="16px" font-weight="700" line-height="24px" font-style="normal" background-color="#2F80ED" color="white" href="#">
                            See results
                        </mj-button>
                    </mj-column>
                </mj-group>
            </mj-section>
        </mj-wrapper>
        
        <mj-include path="./footer.mjml" />

    </mj-body>
</mjml>